<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>SignalR.RabbitMq</id>
    <version>2.2.3</version>
    <authors>Mark deVilliers</authors>
    <owners>Mark deVilliers</owners>
